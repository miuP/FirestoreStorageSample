# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

EXPECTED_FIREBASE_VERSION = '5.0.0'

target 'FirestoreStorageSample' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'Firebase/Storage', "~> #{EXPECTED_FIREBASE_VERSION}"
  pod 'Firebase/Auth', "~> #{EXPECTED_FIREBASE_VERSION}"
  pod 'Firebase/Core', "~> #{EXPECTED_FIREBASE_VERSION}"
  pod 'Firebase/Firestore', "~> #{EXPECTED_FIREBASE_VERSION}"
  pod 'ImageStore'
  pod 'Instantiate'
  pod 'InstantiateStandard'
end
